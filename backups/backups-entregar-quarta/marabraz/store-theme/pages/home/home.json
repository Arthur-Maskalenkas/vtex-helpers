{
  "store.home": {
    "blocks": [
      "cookies-modal",
      "responsive-layout.mobile#home",
      "responsive-layout.desktop#home"
    ]
  },


  "responsive-layout.desktop#home": {
    "title": "Conteúdo Desktop",
    "children": [
      "toggle-layout#banner-special-promotion-desktop",
      "flex-layout.row#deals",
      "flex-layout.row#slider-banners",
      "flex-layout.row#container-banner-with-coupon-desk",
      "flex-layout.row#categories",
      "rich-text#banner-title",
      "flex-layout.row#mosaico-desktop-04",
      "flex-layout.row#banners-desktop-02",
      "flex-layout.row#newsletter-home",
      "flex-layout.row#container-home-app-vitrine-multilevel",
      "flex-layout.row#banners-desktop-03",
      "flex-layout.row#shelf-01",
      "flex-layout.row#banners-desktop-04",
      "flex-layout.row#container--app-limited-offer--home",
      "flex-layout.row#text-seo"
    ]
  },
  "flex-layout.row#container-home-app-vitrine-multilevel": {
    "props": {
      "blockClass": [
        "container__home-app-vitrine-multilevel-row"
      ],
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#container-home-app-vitrine-multilevel"
    ]
  },

  "flex-layout.col#container-home-app-vitrine-multilevel": {
    "props": {
      "blockClass": [
        "container__home-app-vitrine-multilevel-col"
      ],
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "rich-text#vitrine-multilevel",
      "app-vitrine-multilevel-3"
    ]
  },

  "app-vitrine-multilevel-3": {
    "children": [
      "list-context.product-list#vitrine-home12",
      "list-context.product-list#vitrine-home16",
      "list-context.product-list#vitrine-home24",
      "list-context.product-list#vitrine-home36"
    ],
    "props": {
      "tabs": [
        {
          "__editorItemTitle": "Os Melhores Preços"
        },
        {
          "__editorItemTitle": "Envio Imediato"
        },
        {
          "__editorItemTitle": "Mais Vendidos"
        },
        {
          "__editorItemTitle": "Produtos em Destaque"
        }
      ]
    }
  },


  "responsive-layout.mobile#home": {
    "title": "Conteúdo Mobile",
    "children": [
      "toggle-layout#banner-special-promotion",
      "flex-layout.row#deals-mobile",
      "flex-layout.row#slider-banners-mobile",
      "flex-layout.row#banners-mobile-01",
      "flex-layout.row#container-banner-with-coupon-mob",
      "flex-layout.row#categories",
      "rich-text#banner-title",
      "flex-layout.row#banners-mobile-03",
      "flex-layout.row#banners-mobile-04",
      "flex-layout.row#newsletter-home",
      "flex-layout.row#container-home-app-vitrine-multilevel",
      "flex-layout.row#banners-mobile-05",
      "flex-layout.row#container--app-limited-offer--home",
      "flex-layout.row#shelf-01",
      "flex-layout.row#text-seo"
    ]
  }
}
