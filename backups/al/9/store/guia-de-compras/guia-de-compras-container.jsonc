{
  "store.custom#pagina-guia-de-compras": {
    "blocks": [
      "flex-layout.row#container-page-shopping-guide--parent-all-container"
    ]
  },

  "flex-layout.row#container-page-shopping-guide--parent-all-container": {
    "title": "Containers",

    "props": {
      "blockClass": [
        "container__page-shopping-guide--parent-all-container-row",
        "container__page-shopping-guide--parent-row",
        "container__page-shopping-guide-row"
      ],
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#container-page-shopping-guide--parent-all-container"
    ]
  },

  "flex-layout.col#container-page-shopping-guide--parent-all-container": {

    "props": {
      "blockClass": [
        "container__page-shopping-guide--parent-all-container-col",
        "container__page-shopping-guide--parent-col",
        "container__page-shopping-guide-col"
      ],
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.row#container-page-shopping-guide--container--breadcrumb",
      "flex-layout.row#container-page-shopping-guide--section-1--container",
      "flex-layout.row#container-page-shopping-guide--section-3--container",
      "flex-layout.row#container-page-shopping-guide--section-4--container"
    ]
  }
}
