{
  "responsive-layout.desktop#banner": {
    "children": [
      "flex-layout.row#container-institutional-banner"
    ]
  },
  "flex-layout.row#container-institutional-banner": {
    "title": "Container Banner Institucional",
    "props": {
      "fullWidth": true,
      "blockClass": ["container__institutional-banner"]
    },
    "children": ["image#institutional-banner"]
  },

  "image#institutional-banner": {
    "title": "Imagem Banner Institucional",
    "props": {
      "blockClass": "institutional-banner",
      "src": "/arquivos/institutional-banner.png"
    }
  }
}
