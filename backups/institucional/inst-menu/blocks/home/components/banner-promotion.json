{
  "flex-layout.row#home--banner-promotion": {
    "title": "Container banner promoção",
    "props": {
      "blockClass": [
        "container-page-home",
        "grid-1260",
        "no-padding",
        "section-5"


      ],
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "preventVerticalStretch": true,
      "preventHorizontalStretch": true
    },
    "children": [
      "responsive-layout.mobile",
      "responsive-layout.desktop"
    ]
  },

  "responsive-layout.mobile": {
    "children": [
      "image-new-custom#home--mobile--banner-promotion--image-1"
    ]
  },

  "responsive-layout.desktop": {
    "children": [
      "image-new-custom#home--desktop--banner-promotion--image-1"
    ]
  },

  "image-new-custom#home--mobile--banner-promotion--image-1": {
    "title": "Banner Mobile",
    "props": {
      "src": "https://dummyimage.com/600x600/000/fff",
      "height": "450",
      "width": "1260",
      "blockClass": "banner-image"
    }
  },
  "image-new-custom#home--desktop--banner-promotion--image-1": {
    "title": "Banner Desktop",
    "props": {
      "src": "https://dummyimage.com/1260x450/000/fff",
      "height": "450",
      "width": "1260",
      "blockClass": "banner-image"
    }
  }
}
