{
  "flex-layout.row#product-unavailable": {
    "title": "Caso Produto Indisponivel",

    "props": {
      "blockClass": [
        "container__buy-box-product",
        "container__product-unavailable"
      ]
    },

    "children": [
      "flex-layout.col#stack",
      "flex-layout.col#right-col-if-product-unavailable"
    ]
  },

  "flex-layout.col#right-col-if-product-unavailable": {
    "title": "Container Informações Do Produto",

    "props": {
      "preventVerticalStretch": true,
      "blockClass": "container__info-product"

    },

    "children": [
      "flex-layout.row#product-name",
      "flex-layout.row#product-identifier",
      "flex-layout.row#sku-selector",
      "flex-layout.row#availability"
    ]
  }
}
