{
  "vtex.product-highlights@2.x:product-highlights#summary-collection": {
    "title": "Container coleções",
    "props": {
      "type": "collection",
      "blockClass": "summary-collections"
    },
    "children": [ "product-highlight-wrapper#summary-collection" ]
  },

  "product-highlight-wrapper#summary-collection": {
    "props": {
      "blockClass": "summary-collection"
    },
    "children": [
      "product-highlight-text#summary-collection"
    ]
  },

  "product-highlight-text#summary-collection": {
    "props": {
      "message": "{highlightName}"
    }
  }
}
