<keymap version="1" name="Windows copy" parent="$default">
    <!--
        **
        ** botão 1
        **
    -->


    <!--    linha 2-->

    <!--    reformata o codigo -->
    <action id="ReformatCode">
        <keyboard-shortcut first-keystroke="ctrl alt l"/>
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="quote"/>
    </action>

    <!--  refactor this  -->
    <action id="Refactorings.QuickListPopupAction">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="1"/>
    </action>

    <!--    mostrar intenções (remover pai, code style etc.)-->
    <action id="ShowIntentionActions">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="2"/>
    </action>

    <!--    encapsular em um live template-->
    <action id="SurroundWithLiveTemplate">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="3"/>
    </action>

    <!--    manipulação de string-->
    <action id="osmedile.intellij.stringmanip.PopupChoiceAction">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="4"/>
    </action>


    <!--    gerar estrutura (normalmente usado em classes e markdown)-->
    <action id="Generate">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="4"/>
    </action>

    <!--    inserir no markdown-->
    <action id="Markdown.Insert">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="6"/>
    </action>


    <!--//    linha 2-->

    <!--    linha 3-->
    <!--    infos de parametros -->
    <action id="ParameterInfo">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="q"/>
    </action>

    <!--    infos de parametros mais detalhados - caso aperte 2x, abre a doc a direita -->
    <action id="QuickJavaDoc">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="w"/>
    </action>

    <!--    verificar implementação sem sair do arquivo atual -->
    <action id="QuickImplementations">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="e"/>
    </action>
    <!--    linha 3-->

    <!--    linha 4-->

    <!--  ir para arquivo da declaração  -->
    <action id="GotoDeclaration">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="a"/>
    </action>

    <!--    ir para related symbol-->
    <action id="GotoRelated">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="s"/>
    </action>

    <!--//    linha 4-->


    <!--  linha 5-->
    <!--    salvar codigo selecionado como template -->
    <action id="SaveAsTemplate">
        <keyboard-shortcut first-keystroke="shift alt q" second-keystroke="back_slash"/>
    </action>
